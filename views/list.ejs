<%- include('header'); -%>
    <div class="box" id="heading">
      <h1><%= kindOfDay %></h1>
    </div>
    <div class="box">
        <% for( var i = 0; i < listOfItems.length; i++){ %>
         <div class="item">
            <input type="checkbox">
            <p><%= listOfItems[i] %></p>
         </div>  
        <% } %>
      
      <form class="item" action="/" method="post">
        <input type="text" name="nextItem" placeholder="Add new item" autocomplete="off"/>
        <button type="submit">+</button>
      </form>
    
    </div>
<%- include('footer'); -%>

